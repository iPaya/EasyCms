{
  "name": "ipaya/easycms",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.1.0",
    "bower-asset/bootstrap": "4.1.3",
    "vlucas/phpdotenv": "^2.5",
    "yiisoft/yii2": "^2.0.15"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/App",
      "EasyCms\\": "src/EasyCms"
    }
  },
  "require-dev": {
    "yiisoft/yii2-gii": "^2.0"
  }
}
